
#include <iostream>

using namespace std;

int main(){

int ar[20] = {20,34,102,4,6,99,43,23,29,78};
    for (int j = 0; j < 10; j++){
    cout << ar[j];
    if (j>8){
    cout << ".";
    }else{
        cout << ", ";
    }
    
}

cout << endl;

    for (int i = 0; i < 10; i++){
        ar[i]++;
        cout << ar[i];
        if (i>8){
        cout << ".";
        }else{
            cout << ", ";
        }
        
}

cout << endl;
}
